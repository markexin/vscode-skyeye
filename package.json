{
	"name": "memory-doctor",
	"displayName": "Skyeye",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"snippets": [
			{
			  "language": "javascript",
			  "path": "./src/snippets/common.json"
			}
		],
		"commands": [
			{
				"command": "skyeye.quickInput",
				"title": "立即开始👉🏻"
			}
		],
		"views": {
            "memory-doctor": [{
                "id": "memory-doctor",
                "name": "Skyeye"
            }]
        },
		"viewsWelcome": [{
            "view": "memory-doctor",
            "contents": "Skyeye 是一款【E2E】测试和分析框架，用于发现 JavaScript 内存泄漏和优化机会.\n[立即开始](command:skyeye.quickInput)"
        }],
		"viewsContainers": {
			"activitybar": [
			  {
				"id": "memory-doctor",
				"title": "Skyeye 天眼",
				"icon": "media/dep.svg"
			  }
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.46.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"eslint": "^7.6.0",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	}
}
